{
  "name": "Read It Later",
  "description": "Saves URL to a hosted Read It Later instance.",
  "icons": {
      "16": "icon-light.png"
  },
  "version": "0.002",
  "permissions": [
    "activeTab",
    "storage"
  ],
  "options_ui": {
    "page": "options.html",
    "open_in_tab": false
  },
  "browser_action": {
    "default_popup": "default.html",
    "default_icon": "icon-light.png"
  },
  "manifest_version": 2
}
